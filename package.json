{
  "name": "@airmelon-studio/renovate-config",
  "version": "0.1.8",
  "description": "Preset config for renovate",
  "main": "default.json",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write \"*.json\"",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airmelon-studio/renovate-config.git"
  },
  "keywords": [
    "renovate-config"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airmelon-studio/renovate-config/issues"
  },
  "homepage": "https://github.com/airmelon-studio/renovate-config#readme",
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "standard-version": "9.5.0"
  },
  "renovate-config": {
    "default": {
      "labels": [
        "dependencies"
      ],
      "extends": [
        "config:base"
      ],
      "stabilityDays": 7
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
