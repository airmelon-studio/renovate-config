{
  "name": "@airmelon-studio/renovate-config",
  "version": "0.1.6",
  "description": "Preset config for renovate",
  "main": "default.json",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write \"*.json\"",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airmelon-studio/renovate-config.git"
  },
  "keywords": [
    "renovate-config"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airmelon-studio/renovate-config/issues"
  },
  "homepage": "https://github.com/airmelon-studio/renovate-config#readme",
  "devDependencies": {
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "standard-version": "9.1.0"
  },
  "renovate-config": {
    "default": {
      "extends": [
        "config:base"
      ],
      "stabilityDays": 15
    }
